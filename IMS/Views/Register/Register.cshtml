@model IMS.Models.RegisterDB

<form method="post" action="Register">
    <div class="form-group">
        <label for="exampleInputEmail1">Enter the Full name :</label>
        <input type="text" class="form-control" placeholder="Enter name" asp-for="name">
        </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Enter the Address :</label>
        <input type="text" class="form-control"  placeholder="Address" asp-for="address">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Enter the Gender:</label>
        <input type="text" class="form-control" placeholder="Gender" asp-for="gender">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Enter the Contact :</label>
        <input type="text" class="form-control" placeholder="Contact" asp-for="Contact">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Enter the Date of Birth :</label>
        <input type="datetime" class="form-control"  placeholder="Date of Birth" asp-for="Dob">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Enter the  Email :</label>
        <input type="email" class="form-control" placeholder="Email" asp-for="email">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Enter the  Password :</label>
        <input type="password" class="form-control" placeholder="Password" asp-for="password">
    </div>
   
    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Check me out</label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
